    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.24.1",
    "browserify": "^16.2.0",
    "browserify-hmr": "^0.3.6",
    "envify": "^4.1.0",
